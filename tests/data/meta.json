{
    "$schema": "https://raw.githubusercontent.com/moj-analytical-services/etl_manager/master/etl_manager/specs/table_schema.json",
    "name": "test_data",
    "description": "whatevers",
    "data_format": "csv",
    "columns": [
        {
            "name": "col1",
            "type": "character",
            "description": "integers as strings",
            "nullable": false
        },
        {
            "name": "col2",
            "type": "character",
            "description": "categorical variable",
            "enum": ["cat", "dog", "mouse"]
        },
        {
            "name": "col3",
            "type": "character",
            "description": "code that should be 4 letters and 3 numbers seperated by a slash",
            "nullable": true,
            "regex": "[a-zA-Z]{4}/\\d{3}"
        }
    ],
    "location": "cjs_to_ho_offence_lookup/"
}